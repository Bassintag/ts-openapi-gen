{
  "name": "ts-openapi-gen",
  "description": "An easy to use cli to generate typescript from Open API specifications",
  "version": "0.0.2",
  "type": "module",
  "author": "@bassintag",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "prepublish": "yarn build"
  },
  "bin": {
    "tsgen": "dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "dependencies": {
    "chalk": "^5.0.1",
    "commander": "^9.2.0",
    "lodash.get": "^4.4.2",
    "mustache": "^4.2.0",
    "node-fetch": "^3.2.4",
    "ora": "^6.1.0",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.7",
    "@types/mustache": "^4.1.3",
    "@types/node": "^17.0.42",
    "openapi-types": "^11.0.0",
    "typescript": "^4.6.4"
  }
}
