{
  "name": "ts-codegen",
  "description": "An easy to use cli to generate typescript from Open API specifications",
  "version": "1.0.0",
  "type": "module",
  "author": "@bassintag",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch"
  },
  "bin": {
    "tsgen": "dist/index.js"
  },
  "files": [
    "dist", "templates"
  ],
  "dependencies": {
    "chalk": "^5.0.1",
    "commander": "^9.2.0",
    "mustache": "^4.2.0",
    "node-fetch": "^3.2.4",
    "ora": "^6.1.0",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "openapi-types": "^11.0.0",
    "typescript": "^4.6.4",
    "@types/node": "^17.0.42",
    "@types/mustache": "^4.1.3"
  }
}
